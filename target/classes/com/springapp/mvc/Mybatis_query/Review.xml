<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springapp.mvc.DAO.ReviewDao">

       <insert id="makeReview" parameterType="Review" >
              insert into apply_dp(apply_id,user_account,apply_comment,comment_date,user_behavior)
              values(#{apply_id},#{user_account},#{apply_comment},#{comment_date},#{user_behavior})
       </insert>

</mapper>