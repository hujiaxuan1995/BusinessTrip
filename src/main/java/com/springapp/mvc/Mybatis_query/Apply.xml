<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springapp.mvc.DAO.UserDao">

       <select id="insertApply" parameterType="Apply"  resultType="boolean">
              insert into apply(user_id,user_account,user_department,user_apply_time,trip_destination,
              trip_time_begin,trip_time_end,trip_reason,trip_phonecall,manager_comment,high_manager_comment)
              values(#{user_id},#{user_account},#{user_department},#{user_apply_time},#{trip_destination},#{trip_time_begin},
              #{trip_time_end},#{trip_reason},#{trip_phone_call},"","")
       </select>

</mapper>